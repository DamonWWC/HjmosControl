<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:hc="clr-namespace:Hjmos.BaseControls.Controls">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Base/ComboBoxBaseStyle.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <Style x:Key="ComboBoxMemoStyle"  BasedOn="{StaticResource ComboBoxMemoBaseStyle}" TargetType="hc:MemoBox" />
    <Style x:Key="ComboBoxMemoExtentStyle"  BasedOn="{StaticResource ComboBoxMemoExtentBaseStyle}" TargetType="hc:MemoBox" />
    <Style BasedOn="{StaticResource ComboBoxBaseStyle}" TargetType="ComboBox"/>
    <Style BasedOn="{StaticResource TitleComboBoxBaseStyle}" x:Key="TitleComboBox" TargetType="ComboBox"/>
    <Style x:Key="ComboBoxExtend" BasedOn="{StaticResource ComboBoxExtendBaseStyle}" TargetType="ComboBox"/>
</ResourceDictionary>