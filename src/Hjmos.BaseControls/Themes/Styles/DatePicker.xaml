<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:Hjmos.BaseControls.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Base/DatePickerBaseStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource DatePickerBaseStyle}" TargetType="DatePicker" />

    <Style
        x:Key="DatePickerExtend"
        BasedOn="{StaticResource DatePickerExtendBaseStyle}"
        TargetType="DatePicker" />

</ResourceDictionary>